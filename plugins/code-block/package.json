{
  "name": "@yanyu-fe/vuepress-plugin-code-block",
  "version": "0.0.12",
  "description": "vuepress@v2代码实现代码块demo展示",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "scripts": {
    "build": "pnpm clean && pnpm gen && pnpm copy",
    "gen": "tsc -b tsconfig.build.json",
    "gen:w": "tsc -b tsconfig.build.json -w",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.{d.ts,vue,scss,css,less}\" lib",
    "copy:w": "esno script/watchCopy.ts"
  },
  "files": [
    "lib",
    "tscode.png",
    "tsraw.png",
    "tsshow.png"
  ],
  "keywords": [
    "vuepress-plugin"
  ],
  "author": {
    "name": "aibayanyu",
    "email": "aibayanyu@qq.com",
    "url": "https://www.28yanyu.cn"
  },
  "dependencies": {
    "@vuepress/cli": "^2.0.0-beta.35",
    "@vuepress/core": "^2.0.0-beta.35",
    "@vuepress/theme-default": "^2.0.0-beta.35",
    "lodash-es": "^4.17.21",
    "posthtml-parser": "^0.10.1",
    "posthtml-render": "^3.0.0",
    "scss": "^0.2.4",
    "vue": "^3.2.28"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanyu-fe/vuepress-plugins.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanyu-fe/vuepress-plugins/issues"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "@types/lodash-es": "^4.17.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.17",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-dom": "3.2.28",
    "@vuepress/markdown": "^2.0.0-beta.35",
    "@vuepress/plugin-prismjs": "^2.0.0-beta.35",
    "chokidar": "^3.5.2",
    "cpx": "^1.5.0",
    "esno": "^0.13.0",
    "minimist": "^1.2.5",
