{
  "name": "@yanyu-fe/vuepress-theme-dumi",
  "version": "1.0.0-beta.3",
  "description": "Dumi theme of VuePress",
  "keywords": [
    "vuepress-theme",
    "vuepress",
    "theme",
    "default",
    "dumi"
  ],
  "homepage": "https://github.com/yanyu-fe/vuepress-plugins",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanyu-fe/vuepress-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/yanyu-fe/vuepress-plugins/issues"
  },
  "license": "MIT",
  "author": "aibayanyu",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "templates"
  ],
  "scripts": {
    "build": "run-p clean build:node copy",
    "dev": "run-p dev:node copy:w",
    "build:node": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "dev:node": "tsc -b tsconfig.build.json -w",
    "copy:w": "esno script/watchCopy.ts",
    "copy": "cpx \"src/**/*.{d.ts,vue,scss}\" lib"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.35",
    "@vuepress/core": "2.0.0-beta.35",
    "@vuepress/plugin-active-header-links": "2.0.0-beta.35",
    "@vuepress/plugin-back-to-top": "2.0.0-beta.35",
    "@vuepress/plugin-container": "2.0.0-beta.35",
    "@vuepress/plugin-external-link-icon": "2.0.0-beta.36",
    "@vuepress/plugin-git": "2.0.0-beta.35",
    "@vuepress/plugin-medium-zoom": "2.0.0-beta.35",
    "@vuepress/plugin-nprogress": "2.0.0-beta.35",
    "@vuepress/plugin-palette": "2.0.0-beta.36",
    "@vuepress/plugin-prismjs": "2.0.0-beta.36",
    "@vuepress/plugin-theme-data": "2.0.0-beta.36",
    "@vuepress/plugin-toc": "^2.0.0-beta.38",
    "@vuepress/shared": "2.0.0-beta.35",
    "@vuepress/utils": "2.0.0-beta.35",
    "@vueuse/core": "^7.7.1",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "esno": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~4.5.5",
    "webpack": "^5.71.0"
  }
}
